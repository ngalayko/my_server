# galayko.xyz:80
server {
	listen 80;
	server_name galayko.xyz;
	return 301 https://galayko.rocks$request_uri;
}

# galayko.xyz:433
server {
	listen 443 ssl http2; 
	server_name galayko.xyz;

	include snippets/ssl-galayko.xyz.conf;
	include snippets/ssl-params.conf;
	
	return 301 https://galayko.rocks$request_uri;
}
